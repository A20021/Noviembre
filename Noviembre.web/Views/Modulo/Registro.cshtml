<div id="page-wrapper">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Registro de modulo</h1>
            </div>
        </div>

        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-body">
                        <form action="@Url.Action("Guardar", "Modulo")">
                            <div class="form-group">
                                <label>Nombre del modulo:</label>
                                <input class="form-control" name="nombre" />
                                <label>Direccion del modulo:</label>
                                <input class="form-control" name="direccion"/>
                                <label>Horario del modulo:</label>
                                <input class="form-control" name="horario"/>
                                <label>Referencias del modulo:</label>
                                <input class="form-control" name="referencias"/>
                                <label>Id del municipio del modulo:</label>
                                <input class="form-control" name="idMunicipio"/>
                            </div>
                            <button class="btn btn-success" type="submit">Guardar</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
