<div id="page-wrapper">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Registro de cita</h1>
            </div>
        </div>

        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-body">
                        <form action="@Url.Action("Guardar", "Cita")">
                            <div class="form-group">
                                <label>Fecha de la cita:</label>
                                <input class="form-control" name="fecha" />
                                <label>Id del modulo de la cita:</label>
                                <input class="form-control" name="idModulo"/>
                                <label>Id del ciudadano de la cita:</label>
                                <input class="form-control" name="idCiudadano"/>
                                <label>Id del tramite de la cita:</label>
                                <input class="form-control" name="idTramite"/>
                                <label>Id del documento de nacionalidad del ciudadano:</label>
                                <input class="form-control" name="idDocumentoNacionalidad"/>
                                <label>Id del comprobante del domicilio del ciudadano:</label>
                                <input class="form-control" name="idComprobanteDomicilio"/>
                            </div>
                            <button class="btn btn-success" type="submit">Guardar</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

